{
    "name": "react-typed-form-hooks",
    "version": "2.2.3",
    "description": "A performant TypeScript solution for type-safe, complex forms with great UX",
    "author": "Ben Tefay",
    "license": "MIT",
    "repository": "bentefay/react-typed-form-hooks",
    "homepage": "https://github.com/bentefay/react-typed-form-hooks",
    "source": "src/index.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.module.js",
    "exports": {
        "require": "./dist/index.js",
        "default": "./dist/index.modern.mjs"
    },
    "keywords": [
        "react",
        "form",
        "react form",
        "type checked form",
        "typescript",
        "react typescript",
        "typescript form"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "microbundle --no-compress --jsx React.createElement",
        "dev": "microbundle watch --no-compress --jsx React.createElement"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "devDependencies": {
        "@types/node": "16.11.42",
        "@types/react": "18.0.14",
        "@types/react-dom": "18.0.5",
        "microbundle": "0.15.0",
        "prettier": "2.7.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "typescript": "4.7.4"
    },
    "files": [
        "dist"
    ],
    "packageManager": "yarn@3.2.1"
}
